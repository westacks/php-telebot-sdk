{
    "name": "legacy-ua/telebot",
    "description": "Easy way to create Telegram-bots in PHP (with ability to use proxy)",
    "type": "library",
    "homepage": "https://github.com/legacy-ua/telebot",
    "license": "MIT",
    "authors": [
        {
            "name": "LegacyUA",
            "email": "korsair93@gmail.com",
            "homepage": "https://github.com/legacy-ua"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^6.0 || ^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "phpunit/phpunit": "^8.0 || ^9.0 || ^10.0",
        "rector/rector": "^0.17"
    },
    "autoload": {
        "psr-4": {
            "WeStacks\\TeleBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WeStacks\\TeleBot\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "testdox": "phpunit --testdox"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "archive": {
        "exclude": [
            "/.gitignore",
            "/.gitattributes",
            "/.github",
            "/tests",
            "/docs",
            "/phpunit.xml.dist",
            "/rector.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
