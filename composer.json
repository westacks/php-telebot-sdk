{
    "name": "westacks/telebot",
    "description": "Easy way to create Telegram-bots in PHP",
    "type": "library",
    "homepage": "https://github.com/westacks/telebot",
    "license": "MIT",
    "authors": [
        {
            "name": "PunyFlash",
            "email": "puny.flash@gmail.com",
            "homepage": "https://github.com/PunyFlash"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^6.0 || ^7.0"
    },
    "require-dev": {
        "electrolinux/phpquery": "^0.9.7",
        "laravel/pint": "^1.0",
        "league/html-to-markdown": "^5.1",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpunit/phpunit": "^8.0 || ^9.0 || ^10.0",
        "rector/rector": "^0.17",
        "spatie/php-structure-discoverer": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "WeStacks\\TeleBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WeStacks\\TeleBot\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "testdox": "phpunit --testdox"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "archive": {
        "exclude": [
            "/.gitignore",
            "/.gitattributes",
            "/.github",
            "/tests",
            "/docs",
            "/phpunit.xml.dist",
            "/rector.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
